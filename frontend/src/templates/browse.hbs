<table class="table table-striped">
<tr>
<th>Links</th>
<th>World name</th>
<th>User</th>
<th>Area</th>
<th>Visited</th>
<th>Uploaded at</th>
</tr>
{{#each data.maps}}
<tr>
<td>
<a href="/maps/{{ImageName}}">Image</a>
<a href="/map.html?id={{ID}}">Details</a>
</td>
<td>{{WorldName}}</td>
<td><a href="/profile.html?user={{Login}}">{{Login}}</a></td>
<td class='text-nowrap'>{{Area}} ({{Width}}x{{Height}})</td>
<td>{{Visited}}</td>
<td class='text-nowrap'>{{#date}}{{CreatedAt}}{{/date}}</td>
</tr>
{{/each}}
</table>
{{{paginator}}}
